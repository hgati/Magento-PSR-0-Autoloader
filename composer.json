{
    "name": "hgati/hackathon-psr0autoloader",
    "type": "magento-module",
    "description": "This extension adds a PSR-0 autoloader before the Varien autoloader",

    "extra": {
        "map": [
            [
                "app/code/community/Hackathon/PSR0Autoloader",
                "app/code/community/Hackathon/PSR0Autoloader"
            ],
            [
                "app/etc/modules/Hackathon_PSR0Autoloader.xml",
                "app/etc/modules/Hackathon_PSR0Autoloader.xml"
            ],
            [
                "shell/autoloader_initializer.php",
                "shell/autoloader_initializer.php"
            ]
        ]
    }
}
